<?xml version="1.0"?>

<launch>

    <arg name="node_name" default="grav_comp_node" />
    <node pkg="grav_comp" type="grav_comp" name="$(arg node_name)"
          output="screen" cwd="node">

          <rosparam>
            robot_type: "lwr4p_sim"
            CoM_filename: "CoM.yaml"

            pose1: [0.45, 1.1, 1.3, 0.46, 0.12, 0.22, 0.3]
            pose2: [0.45, 1.1, 1.3, 0.46, 0.12, 0.22, 0.3]
            #pose3: [0.45, 1.1, 1.3, 0.46, 0.12, 0.22, 0.3]
            #pose4: [0.45, 1.1, 1.3, 0.46, 0.12, 0.22, 0.3]
            #pose5: [0.45, 1.1, 1.3, 0.46, 0.12, 0.22, 0.3]

          </rosparam>

    </node>

</launch>
